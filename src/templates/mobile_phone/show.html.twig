{% extends 'base.html.twig' %}

{% block title %}MobilePhone{% endblock %}

{% block body %}
    <h1>MobilePhone</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mobile_phone.id }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ mobile_phone.brand }}</td>
            </tr>
            <tr>
                <th>Model</th>
                <td>{{ mobile_phone.model }}</td>
            </tr>
            <tr>
                <th>OperatingSystem</th>
                <td>{{ mobile_phone.operatingSystem }}</td>
            </tr>
            <tr>
                <th>ScreenSize</th>
                <td>{{ mobile_phone.screenSize }}</td>
            </tr>
            <tr>
                <th>Memory</th>
                <td>{{ mobile_phone.memory }}</td>
            </tr>
            <tr>
                <th>Storage</th>
                <td>{{ mobile_phone.storage }}</td>
            </tr>
            <tr>
                <th>Camera</th>
                <td>{{ mobile_phone.camera }}</td>
            </tr>
            <tr>
                <th>BatteryCapacity</th>
                <td>{{ mobile_phone.batteryCapacity }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ mobile_phone.price }}</td>
            </tr>
        </tbody>
